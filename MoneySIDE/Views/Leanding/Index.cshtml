        .card {
            height: 50mm;
            width: 84mm;
            position: absolute;
            margin-top: 370%;
            margin-left: 47%;
            perspective: 800px;
        }

        @@media (max-width: 1024px) {
            .card {
                margin-right: 20%;
                margin-top: 410%;
            }
        }

        .card__side {
            width: 140%;
            height: 140%;
            border-radius: 3.18mm;
            position: absolute;
            backface-visibility: hidden;
            transition: transform 0.7s ease-out;
            cursor: pointer;
            padding: 10px;
        }

        .card__side_front {
            background: linear-gradient(90deg, rgb(0, 0 , 0) 0%, #242424 100%);
            transform: rotateY(0deg);
        }

        .card__side_back {
            background: linear-gradient(-90deg, rgb(0, 0, 0) 0%, #242424 100%);
            transform: rotateY(-180deg);
            color: #eeeeee;
        }

        .card__side__name-bank {
            font-family: Inter, sans-serif;
            font-weight: 500;
            position: relative;
            font-size: 2em;
            margin-left: 4%;
            margin-top: 5%;
            color: white;
        }

        .card__side__name-bank::after {
            content: "Universal Bank";
            position: absolute;
            font-size: 6px;
            top: 105%;
            left: 21%;
            color: #635c77;
        }

        .card__side__name-bank::before {
            content: "₴";
            position: absolute;
            top: 0;
            right: 0;
            color: #635c77;
        }

        .card__side__chip {
            width: 1.9cm;
            height: 1.4cm;
            margin-left: 22px;
            margin-top: -35px;
            background: rgb(226, 175, 35);
            border-radius: 8px;
        }

        .card__side__chip:after {
            content: "";
            display: block;
            position: absolute;
            height: 24px;
            width: 24px;
            top: 80px;
            right: 15px;
            transform: scale(1.3);
        }

        .card__side__name-person {
