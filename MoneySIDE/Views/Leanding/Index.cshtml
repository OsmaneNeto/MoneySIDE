        .container-link__botao {
            color: var(--cor-letra-botao);
            text-decoration: none;
            font-weight: 500;
            font-family: var(--fonte-principal);
        }

        @@media (max-width: 1024px) {
            .cabecalho {
                flex-direction: row;
                align-items: flex-start;
